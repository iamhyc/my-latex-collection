journal = {IEEE Access},
keywords = {Computation offloading,mobile edge computing,optimization,resource management},
pages = {22622--22633},
title = {{Computation Offloading Based on Cooperations of Mobile Edge Computing-Enabled Base Stations}},
volume = {6},
year = {2017}
}

